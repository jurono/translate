# LibreTranslate API Key - Set this to your desired master API key for internal use
LT_API_KEY=your-internal-api-key-here

# Character limit per translation request (0 = no limit)
LT_CHAR_LIMIT=5000

# Coolify will automatically set this - your translation service domain
SERVICE_FQDN_LIBRETRANSLATE=translate.yourdomain.com